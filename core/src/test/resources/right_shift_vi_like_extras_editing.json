{
    "title": "right shift vi like extras editing",
    "rules": [
        {
            "description": "Single quote (right_shift+q to single quote)",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "q",
                        "modifiers": {
                            "mandatory": [
                                "right_shift"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "quote"
                        }
                    ]
                }
            ]
        },
        {
            "description": "Rename (right_shift+r to shift+f6)",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "r",
                        "modifiers": {
                            "mandatory": [
                                "right_shift"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "f6",
                            "modifiers": [
                                "left_shift"
                            ]
                        }
                    ]
                }
            ]
        }
    ]
}